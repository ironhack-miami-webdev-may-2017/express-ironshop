<h2> Our Products </h2>

<ul>
  <% productsAndStuff.forEach((oneProduct) => { %>
    <li>
      <h3> <%= oneProduct.name %> </h3>
      <p> $<%= oneProduct.price %> </p>

                  <!-- req.query.myId -->
      <a href="/products/details?myId=<%= oneProduct._id %>">
        View Details
      </a>
      <br>
               <!-- req.query.myId -->
      <a href="/products/edit?myId=<%= oneProduct._id %>">
        Edit Product
      </a>
      <br>
    </li>
  <% }) %>
</ul>
